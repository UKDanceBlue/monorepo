{
  "compilerOptions": {
    "module": "preserve",
    "moduleResolution": "bundler",
    "paths": {
      "~/assets/*": ["./assets/*"],
      "~/*": ["./src/*"]
    },
    "noUncheckedIndexedAccess": false,
    "exactOptionalPropertyTypes": false,
    "resolvePackageJsonExports": true,
    "target": "es2019",
    "lib": [
      "es2019",
      "es2020.bigint",
      "es2020.date",
      "es2020.number",
      "es2020.promise",
      "es2020.string",
      "es2020.symbol.wellknown",
      "es2021.promise",
      "es2021.string",
      "es2021.weakref",
      "es2022.array",
      "es2022.object",
      "es2022.string"
    ],
    "plugins": [
      {
        "name": "gql.tada/ts-plugin",
        "schema": "../../schema.graphql",
        "tadaOutputLocation": "./src/api/graphql-env.d.ts",
        "shouldCheckForColocatedFragments": false,
        "trackFieldUsage": false
      }
    ]
  },
  "extends": ["../../tsconfig.json", "./node_modules/expo/tsconfig.base.json"],
  "include": [
    "./src",
    "./assets",
    "./app.config.ts",
    "./.expo/types/**/*.ts",
    "./expo-env.d.ts",
    "nativewind-env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "tailwind.config.ts"
  ]
}
