{
  "eslint.workingDirectories": ["."],
  "eslint.useFlatConfig": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "npm.packageManager": "yarn",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never",
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "search.exclude": {
    "package-lock.json": true,
    "yarn.lock": true
  },
  "remote.localPortHost": "allInterfaces",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "sqltools.format": {
    "linesBetweenQueries": 2,
    "reservedWordCase": "upper"
  },
  "eslint.codeActionsOnSave.mode": "problems",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "DanceBlue",
      "database": "danceblue",
      "username": "danceblue",
      "password": "danceblue"
    }
  ],
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "react-native-tools.projectRoot": "./packages/mobile",
  "cSpell.workspaceRootPath": "${workspaceFolder}",
  "dependi.npm.lockFileEnabled": true,
  "workbench.editor.alwaysShowEditorActions": true,
  "javascript.experimental.updateImportsOnPaste": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.experimental.updateImportsOnPaste": true,
  "typescript.experimental.expandableHover": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "tailwindCSS.classAttributes": ["class", "className", "headerClassName"]
}
